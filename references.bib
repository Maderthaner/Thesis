	Number = {7},
	Pages = {705--711},
	Publisher = {Nature Publishing Group},
	Title = {Optically enhanced coherent transport in YBa2Cu3O6.5 by ultrafast redistribution of interlayer coupling},
	Ty = {JOUR},
