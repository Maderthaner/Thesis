	Keywords = {Reflection; Free-electron lasers (FELs); Thin films; Ultrafast measurements; Ultrafast processes in condensed matter, including semiconductors; Surface differential reflectance},
	Month = {May},
	Number = {10},
	Pages = {11396--11406},
	Publisher = {OSA},
	Title = {Temporal cross-correlation of x-ray free electron and optical lasers using soft x-ray pulse induced transient reflectivity},
